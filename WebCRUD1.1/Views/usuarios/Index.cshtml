@model IEnumerable<WebCRUD1._1.Models.usuario>

@{
    ViewBag.Title = "Usuario";
}

<h2>Usuario</h2>

<p>
    @Html.ActionLink("Crear nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.cedula_usuario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.contraseña_usuario)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.cedula_usuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.contraseña_usuario)
        </td>
        
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.cedula_usuario }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.cedula_usuario }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.cedula_usuario })
        </td>
    </tr>
}

</table>
